{
  "version": 1,
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/data/*",
      "headers": [
        {
          "name": "x-content-type-options",
          "value": "nosniff"
        },
        {
          "name": "Content-Type",
          "value": "application/gzip"
        }
      ]
    }
  ],
  "mimeTypes": {
    ".gz": "application/gzip",
    ".json": "application/json"
  }
}
